  "name": "admin2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:admin": "ng serve --app admin --proxy-config proxy.conf.json --host 0.0.0.0 --port 4500 --live-reload-port 49153",
    "start:client": "ng serve --app --aot client --proxy-config proxy.conf.json --host 0.0.0.0 --port 4200 --live-reload-port 49153",
    "build": "ng build --prod",
    "build:admin": "ng build --bh /admin/ --prod --app 1",
    "build:client": "ng build --prod --app 0 && ng build --prod --app 2 --output-hashing=false && npm run webpack:server",
    "build:all": "npm run build:client && npm run build:admin",
    "serve": "node dist/server.js",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "test": "ng test",
    "lint": "ng lint",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e"
  },
